{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ng2-material-dynamic-form",
  "version": "1.0.8",
  "description": "Dynamic form module using material design for angular 2",
  "main": "./src/index",
  "module": "./src/material-import.module",
  "typings": "./src/index.d",
  "repository": {
    "type": "git",
    "url": "https://github.com/rip3rs/ng2-material-dynamic-form"
  },
  "scripts": {
    "pub": "yarn build && yarn publish dist",
    "build": "yarn run build:lib",
    "build:lib": "ng-packagr -p package.json"
  },
  "keywords": [
    "angular2",
    "material",
    "form",
    "dynamic"
  ],
  "author": "“Joseph <“jbriggsr3@gmail.com”> (https://www.jbriggsr3.com)",
  "license": "ISC",
  "dependencies": {
    "ng2-material-import": "^1.3.15"
  },
  "peerDependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/common": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/platform-server": "^7.1.3"
  },
  "devDependencies": {
    "@angular/compiler": "^7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "copyfiles": "^2.1.0",
    "ng-packagr": "^4.4.5",
    "rxjs": "^6.2.2",
    "tsickle": "^0.34.0",
    "typescript": ">=3.1.1 <3.2",
    "zone.js": "^0.8.26"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "whitelistedNonPeerDependencies": [
      "ng2-material-import"
    ]
  }
}
