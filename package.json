{
  "name": "ng2-material-dynamic-form",
  "version": "1.0.8",
  "description": "Dynamic form module using material design for angular 2",
  "main": "./dist/index",
  "module": "./dist/material-import.module",
  "typings": "./dist/index.d",
  "repository": {
    "type": "git",
    "url": "https://github.com/rip3rs/ng2-material-dynamic-form"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "rm -rf dist && yarn run prepack:html && yarn run prepack:tsc",
    "prepack:tsc": "tsc -p tsconfig.json",
    "prepack:html": "cd src && copyfiles **/*.html **/*.css ../dist/"
  },
  "keywords": [
    "angular2",
    "material",
    "form",
    "dynamic"
  ],
  "author": "“Joseph <“jbriggsr3@gmail.com”> (https://www.jbriggsr3.com)",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/common": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/platform-server": "^7.1.3",
    "ng2-material-import": "^1.3.0"
  },
  "devDependencies": {
    "@angular/compiler": "^7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "copyfiles": "^2.1.0",
    "rxjs": "^6.2.2",
    "typescript": ">=3.1.1 <3.2",
    "zone.js": "^0.8.26"
  }
}
